#!/usr/bin/env bash

source ../config

EXEC_DIR=$BUILD_DIR/transport

SOCKET_WRAPPER_LIBRARY_NAME=libsockerwarpper.so
SOCKET_WRAPPER_LIBRARY_PATH="$EXEC_DIR/$SOCKET_WRAPPER_LIBRARY_NAME"

DCCS_SOCKET_LIBRARY_NAME=libdccssocket.so
DCCS_SOCKET_LIBRARY_PATH="$EXEC_DIR/$DCCS_SOCKET_LIBRARY_NAME"

MPI_DAEMON_EXECNAME=mpi_daemon
MPI_DAEMON_EXECPATH="$EXEC_DIR/$MPI_DAEMON_EXECNAME"

